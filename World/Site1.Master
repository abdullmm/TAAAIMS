               <a href="Purchase.aspx" class="menu">Purchase</a>
               <a href="ActivateUnit.aspx" class="menu">Activate</a>
               <asp:LoginStatus ID="LoginStatus1" runat="server" CssClass="menu"/>
               
               </AnonymousTemplate>
               <LoggedInTemplate>
